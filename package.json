{
  "name": "printvisionbolt",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/dashboard",
    "apps/blog",
    "apps/stores/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rimraf .turbo node_modules **/node_modules",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "prettier": "^3.1.0",
    "turbo": "^2.4.4",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.6.0"
}