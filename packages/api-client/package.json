{
  "name": "@printvisionbolt/api-client",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "typecheck": "tsc --noEmit",
    "typecheck:test": "tsc -p tsconfig.test.json --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@printvisionbolt/api": "workspace:*",
    "@printvisionbolt/api-types": "workspace:*",
    "@tanstack/react-query": "^5.0.0",
    "@trpc/client": "^10.38.0",
    "@trpc/react-query": "^10.38.0",
    "@trpc/server": "^10.38.0",
    "superjson": "^2.2.1"
  },
  "devDependencies": {
    "@printvisionbolt/typescript-config": "workspace:*",
    "@printvisionbolt/eslint-config": "workspace:*",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.34.0",
    "jsdom": "^22.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}